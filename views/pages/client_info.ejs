<div class="my-container" id="loading">
    <h1>Your Infomation</h1><br/>
    <h2>Software:</h2><br/>
    <h3 id="software"></h3><br/>

    <h2>IP Adress:</h2><br/>
    <h3 id="ipaddress"></h3><br/>

    <h2>Language:</h2>
    <h3 id="language"></h3>

</div>
<script>
    fetch("https://vuhuykhoi.herokuapp.com/api/whoami")
        .then(res => res.json())
        .then(data => {
            console.log(data);
            document.getElementById("software").innerHTML = data.software;
            document.getElementById("ipaddress").innerHTML = data.ipaddress;
            document.getElementById("language").innerHTML = data.language;
        })
        .catch(err => {
            console.log(JSON.stringify(err));
        });
</script>